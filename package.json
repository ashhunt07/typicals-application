        "favicon32": "img/icons/favicon-32x32.png",
        "favicon16": "img/icons/favicon-16x16.png",
        "appleTouchIcon": "./img/icons/apple-icon-152x152.png",
        "msTitleImage": "./img/icons/ms-icon-144x144.png"
