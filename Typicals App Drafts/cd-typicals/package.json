{
  "name": "cd-typicals",
  "version": "1.0.0",
  "description": "Clear Design Typicals App",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'public/*'\" \"npm run public\"",
    "client": "cd public && npm run start",
    "server": "nodemon server.js --ignore public",
    "build": "cd public && npm run build",
    "install": "cd public && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "vue": "^3.0.11"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.7"
  }
}
